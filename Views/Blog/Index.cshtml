@model IEnumerable<BlogPostItemViewModel>

<form asp-route-returnurl="@ViewData["ReturnUrl"]" asp-route="AddPost" method="post">
    <div class="card mt-0 mb-0">
        <div class="card-body">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group bmd-form-group">
                <ul>
                    @foreach (var item in Model)
                    {
                    <li>
                        <h1>@item.Title</h1>
                        @item.Data

                    </li>
                    }
                </ul>
               
            </div>
            
        </div>
    </div>
</form>
